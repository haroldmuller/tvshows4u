<p id="notice"><%= notice %></p>

<h1>Mailingl Forms</h1>

<table>
  <thead>
    <tr>
      <th>Subject</th>
      <th>Body</th>
      <th>To</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @mailingl_forms.each do |mailingl_form| %>
      <tr>
        <td><%= mailingl_form.subject %></td>
        <td><%= mailingl_form.body %></td>
        <td><%= mailingl_form.to %></td>
        <td><%= link_to 'Show', mailingl_form %></td>
        <td><%= link_to 'Edit', edit_mailingl_form_path(mailingl_form) %></td>
        <td><%= link_to 'Destroy', mailingl_form, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Mailingl Form', new_mailingl_form_path %>
